    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.19.0",
    "denodeify": "1.2.1",
    "docma": "1.5.1",
    "download-cli": "1.0.1",
